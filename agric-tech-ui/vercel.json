{
  "builds": [
    {
      "src": "agric-tech-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "installCommand": "npm install --prefix agric-tech-ui",
  "buildCommand": "npm run build --prefix agric-tech-ui",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/agric-tech-ui/dist/index.html"
    }
  ]
}
